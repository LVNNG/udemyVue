AC,CAAC,CAAC,CAAC,CAAC;AACpB","file":"/Users/elvinng/Documents/ElvinNg/UdemyVue/main-project/src/pages/requests/RequestsReceived.vue","sourceRoot":"","sourcesContent":["<template>\n<base-dialog :show=\"!!error\" title=\"an error occured\" @close=\"handleError\"> \n  <p>{{error}}</p>\n</base-dialog>\n<base-card>\n  <header>\n    <h1>Requests Received</h1>\n  </header>\n  <div v-if=\"isLoading\">\n        <base-spinner></base-spinner>\n  </div>\n  <ul>\n    <request-item v-for=\"request in requests\" :key=\"request.id\" :id=\"request.id\" :coachId=\"request.coachId\" :userEmail=\"request.userEmail\" :message=\"request.message\"></request-item>\n  </ul>\n  </base-card>\n</template>\n<script>\nimport RequestItem from '../../components/requests/RequestItem.vue'\nexport default {\n  data() {\n    return {\n      isLoading: false,\n      error: ''\n    }\n  },\n\n  components: {\n    RequestItem\n  },\n  computed: {\n    requests(){\n      const requestData = this.$store.getters['requests/requests']\n      return requestData\n    }\n  }, \n  methods: {\n    async loadRequests(){\n      this.isLoading = true\n      try {\n      await this.$store.dispatch('requests/setRequests')\n      } catch(error){\n        this.error = error.message || 'Something went wrong'\n      }\n      this.isLoading = false\n    },\n     handleError() {\n      this.error = null\n    }\n  },\n  created() {\n    this.loadRequests()\n  }\n\n}\n</script>\n<style>\nheader {\n  text-align: center;\n}\n\nul {\n  list-style: none;\n  margin: 2rem auto;\n  padding: 0;\n  max-width: 30rem;\n}\n\nh3 {\n  text-align: center;\n}\n</style>"]}]}