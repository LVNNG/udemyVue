>\n<script>\nexport default {\n    emits: ['change-filter'],\n    data(){\n        return {\n            filters: {\n                frontend: true,\n                backend: true,\n                career: true\n            }\n        }\n    },\n    methods: {\n        setFilter(event) {\n            const inputId = event.target.id;\n            const isActive = event.target.checked;\n            const updatedFilters = {\n                ...this.filters,\n                [inputId]: isActive\n            };\n            this.filters = updatedFilters\n            console.log(updatedFilters)\n            this.$emit('change-filter', updatedFilters)\n        }\n    }\n}\n</script>\n"]}]}